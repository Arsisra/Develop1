<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_loc_tem_tree" model="ir.ui.view">
			<field name="name">loc.tem.tree</field>
			<field name="model">loc.tem</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="Data Divisi">
					<field name="name"/>
                    <field name="tempat"/>
				</tree>
			</field>
		</record>
	
		<record id="view_loc_tem_form" model="ir.ui.view">
			<field name="name">loc.tem.from</field>
			<field name="model">loc.tem</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="Data Divisi">
					<sheet>
						<!-- <div class="oe_title">
							<label form="name" class="oe_edit_only"
									string="Data Divisi"/>
						</div> -->	
						<group>
							<group>
								<field name="name"/>
                                <field name="tempat"/>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>
	</data>
</odoo>